var r: int = 0;

function circleArea(r: int): float {
    val area = 3.14 * r * r;
    return area;
}

function interruptForAndReturn(): String {
    for (var i = 0; i < 114514; i=i+1) {
        for (var j = 0; j < i; j=j+1) {
            if (i == 91 && j == 78) return i + "" + j;
        }
    }
}

function main() {
    r = 4;
    println("r=" + r + ", area=" + circleArea(r)); // exception here
    r = 5;
    println("r=" + r + ", area=" + circleArea(r));

    if (2 < 1) println("");
    else if (3 < 1) println("");
    else println("huh");

    val negative = -1;

    println("negative: " + negative);

    val interrupted = interruptForAndReturn();
    println(interrupted);
}
