var r: int = 0;

function circleArea(r: int): float {
    val area = 3.14 * r * r;
    return area;
}

function interruptForAndReturn(): String {
    for (var i = 0; i < 114514; i=i+1) {
        for (var j = 0; j < i; j=j+1) {
            if (i == 91 && j == 78) return i + "" + j;
        }
    }
}

function fib(a: int): int {
    if (a <= 1)
        return 1;
    return fib(a - 1) + fib(a - 2);
}

function fib1(a: int): int {
     if (a <= 1) return 1;
     val f1 = fib1(a - 1);
     val f2 = fib1(a - 2);
     return f1 + f2;
}

function returns(a: Any): Any {
    return a;
}

