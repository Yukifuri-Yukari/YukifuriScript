function fib(a: int): int {
    if (a == 0) return 0;
    if (a <= 1) return 1;
    return fib(a - 1) + fib(a - 2);
}

function main() {
    println("Calculating fib(0) to fib(20): ");
    for (var i = 0; i<31; i=i+1) {
        println("fib(" + i + "): " + fib(i));
    }
